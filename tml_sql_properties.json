{
    "auto_binding": "ON",
    "dependency_health_check": "OFF",
    "data_directory": "/var/lib/mysql",
    "expire_logs_days": 30,
    "innodb_buffer_pool_size": "128M",
    "key_buffer_size": "16M",
    "mysql_bin": "mysql-bin",
    "mysql_log_error": "/var/log/mysqld.log",
    "mysql_log_error_verbosity": 1,
    "max_allowed_packet": 16777216,
    "max_binlog_size": 1073741824,
    "max_connections": 500,
    "mom_connect_timeout": 30000,
    "mom_read_timeout": 120000,
    "mom_download_and_sync":false,
    "query_cache_size": "8M",
    "token_sync_interval_minutes": 15,
    "service_sync_interval_minutes": 15,
    "developer_sync_interval_minutes": 15,
    "table_open_cache": 1000
}
